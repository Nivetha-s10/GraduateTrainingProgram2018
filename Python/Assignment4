class shapes:

    def area_rect(self,length,breadth):
		if(length>0 and breadth>0):
            return length * breadth
		else:
		    return "no negative inputs"
	
    def area_circle(self,radius):
	    if(radius>0)
            return 3.14*radius*radius
		else:
		    return "no negative inputs"
		

